{
    "name": "js-grammar-fix",
    "displayName": "Javascript Grammar Fix",
    "description": "Bring power back into the hands of console and type constructors",
    "icon": "./src/icon.png",
    "author": "Robyn",
    "repository": {
        "type": "git",
        "url": "https://github.com/Azuriru/js-grammar-fix"
    },
    "version": "1.0.0",
    "engines": {
        "vscode": "0.10.x"
    },
    "categories": [
        "Programming Languages",
        "Other",
        "Visualization"
    ],
    "keywords": [
        "js",
        "jsx",
        "grammar"
    ],
    "contributes": {
        "grammars": [
            {
                "language": "javascriptreact",
                "scopeName": "source.js.jsx",
                "path": "./src/JavaScriptReact.tmLanguage.json",
                "embeddedLanguages": {
                    "meta.tag.js": "jsx-tags",
                    "meta.tag.without-attributes.js": "jsx-tags",
                    "meta.tag.attributes.js.jsx": "javascriptreact",
                    "meta.embedded.expression.js": "javascriptreact"
                },
                "tokenTypes": {
                    "meta.template.expression": "other",
                    "meta.template.expression string": "string",
                    "meta.template.expression comment": "comment",
                    "entity.name.type.instance.jsdoc": "other",
                    "entity.name.function.tagged-template": "other",
                    "meta.import string.quoted": "other",
                    "variable.other.jsdoc": "other"
                }
            },
            {
                "language": "javascript",
                "scopeName": "source.js",
                "path": "./src/JavaScript.tmLanguage.json",
                "embeddedLanguages": {
                    "meta.tag.js": "jsx-tags",
                    "meta.tag.without-attributes.js": "jsx-tags",
                    "meta.tag.attributes.js": "javascript",
                    "meta.embedded.expression.js": "javascript"
                },
                "tokenTypes": {
                    "meta.template.expression": "other",
                    "meta.template.expression string": "string",
                    "meta.template.expression comment": "comment",
                    "entity.name.type.instance.jsdoc": "other",
                    "entity.name.function.tagged-template": "other",
                    "meta.import string.quoted": "other",
                    "variable.other.jsdoc": "other"
                }
            }
        ]
    }
}